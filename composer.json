{
    "name": "headwires/translator-client",
    "description": "Laravel client package for syncing translations from Headwires Translator CDN",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Headwires",
            "email": "info@headwires.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Headwires\\TranslatorClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Headwires\\TranslatorClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Headwires\\TranslatorClient\\TranslatorClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
